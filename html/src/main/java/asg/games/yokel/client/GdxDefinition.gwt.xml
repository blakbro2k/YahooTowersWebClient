<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.8.2//EN" "http://www.gwtproject.org/doctype/2.8.2/gwt-module.dtd">
<module rename-to="html">
    <source path="" />
	<inherits name="com.badlogic.gdx.backends.gdx_backends_gwt" />
	<inherits name="asg.games.yokel.client.YahooTowersClient" />
	<inherits name="com.github.czyzby.autumn.gwt.GdxAutumnGwt" />
	<inherits name="com.github.czyzby.autumn.mvc.GdxAutumnMvc" />
	<inherits name="com.github.czyzby.websocket.GdxWebSocket" />
	<inherits name="com.github.czyzby.websocket.GdxWebSocketGwt" />
	<inherits name="com.github.czyzby.kiwi.GdxKiwi" />
	<inherits name="com.github.czyzby.lml.GdxLml" />
	<inherits name="com.github.czyzby.lml.vis.GdxLmlVis" />
	<inherits name="com.kotcrab.vis.vis-ui" />
	<inherits name="com.rafaskoberg.gdx.typinglabel.typinglabel" />
	<inherits name="regexodus" />
    <entry-point class="asg.games.yokel.client.gwt.GwtLauncher" />
    <set-configuration-property name="gdx.assetpath" value="../assets" />
    <set-configuration-property name="xsiframe.failIfScriptTag" value="FALSE"/>
    <!-- These two lines reduce the work GWT has to do during compilation and also shrink output size. -->
    <set-property name="user.agent" value="gecko1_8, safari"/>
    <collapse-property name="user.agent" values="*" />
    <!-- Remove the "user.agent" lines above if you encounter issues with Safari or other Gecko browsers. -->

	<!-- This can be used to make CssResource produce human-readable CSS -->
	<set-configuration-property name="CssResource.style" value="DETAILED" />
</module>