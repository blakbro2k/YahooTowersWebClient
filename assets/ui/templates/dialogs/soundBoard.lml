<dialog id="soundBoard" title="Sound Board" style="dialog" onecolumn="false" closebutton="true">
    <:each soundId="$getAllSoundIds">
        <textButton id="{soundId}" row="$checkRow" onclick="playSound">@{soundId}</textButton>
    </:each>

    <!-- Fallback: Add <cell/> to complete final row if needed -->
    <:for i="0 to $padFinalRow - 1">
        <actor />
    </:for>
</dialog>